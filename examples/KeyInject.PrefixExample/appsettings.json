{
  "key-1": "from env: ${pre_user_name}",
  "key-2": "from env: {{PRE_USER_NAME}}",
  "key-3": "from env: $<pRe_uSer_nAme>",
  "key-4": "from env: <<PRE_USER_NAME>>",
  "key-5": "from env: !{pre_user_name}!",
  "key-6": "from env: %PRE_USER_NAME%",
  "KeyInject": {
    "Enabled": true,
    "IgnoreCase": true,
    "ReplaceRepeatCount": 5,
    "Patterns": [
      "${_}", "{{_}}", 
      "$<_>", "<<_>>",
      "!{_}!", "%_%"
    ],
    "KeyPrefixes": [
      "PRE_", "ANOTHER_"
    ]
  }
}
